{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "types": [],
    "rootDir": "src",
    "outDir": "lib",
    "noImplicitThis": false,
    "strictFunctionTypes": false,
    },
    "include": [
      "src",
    ]
  }